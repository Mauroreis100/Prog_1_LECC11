package Exercicio_2;

import java.util.*;

public class Programa {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
	
		System.out.println("Insira um número:");
		int n = input.nextInt();
	
		Vector vetor = new Vector(n);
		
		//Preencher o vector
		System.out.println("Insira os números:");
		for(int i=0;i<vetor.size();i++) {
			double numero=input.nextDouble();
			vetor.add(numero);
		}
		((Object) vetor).getMenorMaior();
	}
	public static void getMenorMaior(Vector vetor) {
		double maior= (double)vetor.get(0);
		double menor= (double)vetor.get(0);
		for(int i=0;i<vetor.size();i++) {
			if((double)vetor.get(i)>maior) {
				maior=(double)vetor.get(i);
			}else {
				menor=(double)vetor.get(i);
			}
		}
		System.out.println("O maior número é: "+maior);
		System.out.println("O menor número é: "+menor);
	}
}
