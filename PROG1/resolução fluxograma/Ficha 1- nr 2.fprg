                    <output expression="nota2" newline="True"/>
                    <output expression="media" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Reprovado com primeira nota, segunda nota e media: &quot;" newline="True"/>
                    <output expression="nota1" newline="True"/>
                    <output expression="nota2" newline="True"/>
                    <output expression="media" newline="True"/>
                </else>
            </if>
        </body>
