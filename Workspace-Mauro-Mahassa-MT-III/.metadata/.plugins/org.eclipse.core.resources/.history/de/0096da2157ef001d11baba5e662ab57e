package relatorio;
import implementar.Salario;
import funcionario.Funcionario;
import funcionarios.*;
import java.util.*;

public class Empresa {
	private int tamanho=0;
private Funcionario f[] = new Funcionario[5];

public Funcionario[] getF() {
	return f;
}

public void add(Funcionario fun) {
	f[tamanho]=fun;
	tamanho++;
}
		
}
